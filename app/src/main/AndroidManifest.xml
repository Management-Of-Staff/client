<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="myapplication.second.workinghourmanagement">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/Theme.WorkingHourManagement"
        tools:targetApi="31">
        <activity
            android:name=".store.OwnerSearchPrimaryAddressActivity"
            android:exported="false" />
        <activity
            android:name=".store.OwnerStoreConversionActivity"
            android:exported="false" />
        <activity
            android:name=".store.OwnerStoreRegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".store.OwnerStoreInfoModificationActivity"
            android:exported="false" />
        <activity
            android:name=".member.MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".member.FindPasswdActivity"
            android:exported="true" />
        <activity
            android:name=".member.ResetPasswdActivity"
            android:exported="true" />
        <activity
            android:name=".member.OwnerTosActivity"
            android:exported="true" />
        <activity
            android:name=".member.OwnerJoinActivity"
            android:exported="true" />
        <activity
            android:name=".member.OwnerJoinInfoActivity"
            android:exported="true" />
        <activity
            android:name=".member.OwnerJoinCompleteActivity"
            android:exported="true" />
        <activity
            android:name=".member.StaffJoinActivity"
            android:exported="true" />
        <activity
            android:name=".member.StaffTosActivity"
            android:exported="true" />
        <activity
            android:name=".home.OwnerHomeActivity"
            android:exported="true" />
        <activity
            android:name=".profile.OwnerProfileInfoActivity"
            android:exported="true" />
    </application>
</manifest>